{
	"manifest_version":2,
	
	"name":"SelAssist",
	
	"version":"0.0.3.5",
	
	"minimum_chrome_version": "10.0",
		
	"description":"Find all HTML element properties required for Selenium Webdriver automation",
	
	"permissions" : [
        "activeTab",
        "tabs",
		"clipboardWrite",
		"<all_urls>"
	],

	"icons":{
    	"128": "Images/se_big-logo.png"
	},

	"devtools_page": "devtools.html"	
}
